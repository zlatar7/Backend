<main class="flex-grow-1 p-5 m-auto my-3 container" style="z-index: -1;">
  <h1 style="text-align:center; color: #00378a; margin-top: 2em">{{title}}</h1>
  
  <div class="container mt-4">
  <div class="row p-4 m-4">
    {{#each products}}
    <div class="col md-4">
      <div class="card">
        <img src="{{this.photo}}" class="card-img-top" alt="product" height="250">
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">$ {{this.price}}</p>
          <p class="card-text">En stock: {{this.stock}}</p>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
  </div>
 
</main>