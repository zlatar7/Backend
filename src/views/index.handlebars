<main class="flex-grow-1 p-5 m-auto text-center container" style="z-index: -1;">
  
  <h1 style="margin: 2em auto 1em auto; color: #00378a; width: fit-content;" class="rounded-3 bg-light bg-opacity-50 p-1">{{title}}</h1>
  
  <div class="container mt-4">
  <div class="row row-cols-4 p-4">
    {{#each products}}
    <div class="col md-4 p-4">
      <div class="card p-1 bg-white bg-opacity-75 rounded-3 border-primary border-1">
        <img src="{{this.photo}}" class="card-img-top" alt="product" height="250">
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">$ {{this.price}}</p>
          <p class="card-text">En stock: {{this.stock}}</p>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
  </div>
 
</main>